includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon

parameters:
    level: 5
    paths:
        - src

    reportUnmatchedIgnoredErrors: false

    ignoreErrors:
        # Add specific error patterns to ignore if needed
        - '#PHPDoc tag @var#'
        - 
            identifier: missingType.iterableValue
        -
            identifier: missingType.generics 